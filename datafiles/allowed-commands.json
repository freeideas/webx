{
    "_comment": "DEPRECATED: This file is no longer used. OS command validation is now handled by DefaultSecurityGuard.",
    "allowedCommands": {
        "echo": {
            "executable": "/bin/echo",
            "allowAllArgs": true,
            "maxTimeout": 1000
        },
        "ls": {
            "executable": "/bin/ls",
            "allowedArgs": ["^-[la]+$", "^/tmp/.*", "^\\./.*"],
            "maxTimeout": 5000
        },
        "date": {
            "executable": "/bin/date",
            "allowAllArgs": true,
            "maxTimeout": 1000
        },
        "pwd": {
            "executable": "/bin/pwd",
            "allowAllArgs": false,
            "maxTimeout": 1000
        },
        "whoami": {
            "executable": "/usr/bin/whoami",
            "allowAllArgs": false,
            "maxTimeout": 1000
        },
        "claude": {
            "executable": "/home/ace/.npm-global/bin/claude",
            "allowAllArgs": true,
            "maxTimeout": 120000
        },
        "cd": {
            "executable": "/bin/bash",
            "allowedArgs": ["^-c$", "^cd .*"],
            "maxTimeout": 1000
        }
    },
    "defaultTimeout": 3000,
    "maxConcurrentCommands": 10
}